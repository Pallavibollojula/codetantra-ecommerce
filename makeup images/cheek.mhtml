<script type="text/javascript">
require(['jquery'], function($){
				$(document).ready(function(e) {
$('.sticky-header').addClass('page-headers');
var didScroll;
var lastScrollTop = 0;
var delta = 5;
var navbarHeight = $('.sticky-header').outerHeight();

$(window).scroll(function(event){
didScroll = true;
});

setInterval(function() {
if (didScroll) {
	hasScrolled();
	didScroll = false;
}
}, 50);

function hasScrolled() {
var st = $(this).scrollTop();
if(Math.abs(lastScrollTop - st) <= delta)
	return;
if (st > lastScrollTop && st > navbarHeight){
	// Scroll Down
	$('.sticky-header').removeClass('page-headers').addClass('page-header-up');
} else {
	// Scroll Up
	if(st + $(window).height() < $(document).height()) {
		$('.sticky-header').removeClass('page-header-up').addClass('page-headers');
	}
}

lastScrollTop = st;
}
});
				
});					
</script>
