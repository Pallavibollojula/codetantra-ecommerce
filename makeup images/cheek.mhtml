					  success: function(data){
						 $(".search-suggestions-wrap").html(data);
						 $(".search-suggestions-wrap").show(); 
						  $( "li.search-keysuggestion" ).mouseover(function(e) {

									// For the boolean value
									var boolKey = $(this).html();
									getSuggest(boolKey);
							        $('form#search_mini_form').find('#search').val(boolKey);
							});
						   $( "li.search-keysuggestion" ).click(function(e) {

									// For the boolean value
									var boolKey = $(this).html();
									getClickSuggest(boolKey);
							        $('form#search_mini_form').find('#search').val(boolKey);
							});
						  return true;
					  }
					});		
							
				 }
			});	
			$('form#msearch_mini_form').on('keyup change keydown',function(e){
				
				//alert(1);
				var searchQuery = $(this).find('#search').val();
