									getClickSuggest(boolKey);
							        $('form#msearch_mini_form').find('#search').val(boolKey);
							});
						  return true;
					  }
					});		
							
				 }
			});		
			$("#close_search_btn").click(function(){
				$('#search_mini_form').toggleClass('active');
				
			});
		   function getSuggest(key){
			   
					$.ajax({
					  type: "GET",
					  url: "/extsearch/autosuggest/prosuggest/",
					  data:{q:key},
					  cache: false,
					  success: function(sdata){
						  $("#pro-search-suggest").html('');
						  
						  var obj = jQuery.parseJSON(sdata);
						  console.log(sdata.proids);
						 $("#pro-search-suggest").html(obj.html);
						 //$("#search_suggest").show();
						 
						  return true;
					  }
					});		
							
				 
		   }
		   function getClickSuggest(key){
			   
					$.ajax({
					  type: "GET",
					  url: "/extsearch/autosuggest/prosuggest/",
					  data:{q:key},
					  cache: false,
					  success: function(sdata){
						  //$("#pro-search-suggest").html('');
						  
